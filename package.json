{
  "name": "the-playground",
  "version": "1.0.0",
  "description": "A playground for testing and/or experimenting with react library",
  "main": "main.js",
  "scripts": {
    "start": "webpack-dev-server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-server": "nodemon App/server.js --exec babel-node --presets react,es2015,stage-0 --watch",
    "build": "babel  App -d dist --presets react,es2015,stage-0",
    "serve": "node dist/server.js",
    "babel": "babel",
    "webpack": "webpack --watch",
    "sync-assets": "cp -r ./App/sass/* node_modules/bootstrap-sass/assets/stylesheets;"
  },
  "keywords": [
    "playground",
    "sandbox"
  ],
  "author": "Arga Aditya",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.26.0",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "ejs": "^2.5.7",
    "express": "^4.15.4",
    "prop-types": "^15.5.10",
    "react": "16.0.0",
    "react-bootstrap": "^0.31.2",
    "react-dom": "16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^4.4.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-register": "^6.26.0",
    "bootstrap-sass": "^3.3.7",
    "bootstrap-sass-loader": "^1.0.10",
    "css-loader": "^0.28.7",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "file-loader": "^0.11.2",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^1.3.1",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2"
  }
}
